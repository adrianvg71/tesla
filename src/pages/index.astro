---
import Section from '../components/ModelYSection.astro';
import HeroSection from '../components/HeroSection.astro';
import LandingHeader from '../components/LandingHeader.astro';
import Layout from '../layouts/Layout.astro';
import ModelYSection from '../components/ModelYSection.astro';
import ModelXSection from '../components/ModelXSection.astro';
---

<Layout title="Tesla">
	<LandingHeader />
	<main class="snap-y snap-mandatory relative w-full h-screen overflow-y-auto overflow-x-hidden scroll-smooth">
		<div class="snap-center">
			<HeroSection />
		</div>
		<div class="snap-center">
			<ModelYSection />
		</div>
		<div class="snap-center">
			<ModelXSection />
		</div>
	</main>
</Layout>